# Environment variables untuk Docker Compose

# Application Port (public facing)
APP_PORT=8000

# Database Configuration
DB_DATABASE=form_vaksin
DB_USERNAME=formvaksin_user
DB_PASSWORD=ChangeThisPassword123!
DB_ROOT_PASSWORD=ChangeRootPassword123!
DB_PORT=3307  # Ganti ke 3307 untuk avoid conflict dengan MySQL lain

# PHPMyAdmin Port (optional, bisa dimatikan di production)
# Ganti ke 8081 karena 8080 sudah dipakai
PMA_PORT=8081
